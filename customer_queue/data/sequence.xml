<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="sequence_customer" model="ir.sequence">
            <field name="name">customer sequence</field>
            <field name="code">customer.sequence</field>
            <field name="prefix">TOKEN</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_counter" model="ir.sequence">
            <field name="name">counter sequence</field>
            <field name="code">counter.sequence</field>
            <field name="prefix">COUNTER</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>